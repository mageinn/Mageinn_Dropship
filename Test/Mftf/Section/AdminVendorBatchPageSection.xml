<?xml version="1.0" encoding="UTF-8"?>

<sections xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:noNamespaceSchemaLocation="../../../../../../vendor/magento/magento2-functional-testing-framework/src/Magento/FunctionalTestingFramework/Page/etc/SectionObject.xsd">
    <section name="AdminExportBatchPage">
        <element name="vendor" type="select" selector="select[name='vendor_batches[vendor_name]']"/>
        <element name="saveButton" type="button" selector=".save"/>
        <element name="filters" type="button" selector="button[data-action='grid-filter-expand']"/>
        <element name="applyFilters" type="button" selector="button[data-action='grid-filter-apply']"/>
        <element name="clearFilters" type="button" selector=".admin__data-grid-header .action-clear"/>
        <element name="batchVendorFilter" type="select" selector=".admin__data-grid-filters select[name='vendor_id']"/>
        <element name="batchTypeFilter" type="select" selector=".admin__data-grid-filters select[name='type']"/>
        <element name="batchStatusFilter" type="select" selector=".admin__data-grid-filters select[name='status']"/>
        <element name="batchExportVendorNameInput" type="input" selector="input[name='batch_data[vendor]']"/>
        <element name="batchExportStatusInput" type="select" selector="input[name='batch_data[status]']"/>
        <element name="selectOption" type="button" selector="button.action-multicheck-toggle"/>
        <element name="selectAll" type="button" selector="div.action-multicheck-wrap._active > ul.action-menu > li > span.action-menu-item"/>
        <element name="actionsOption" type="button" selector="button.action-select"/>
        <element name="deleteBatch" type="button" selector="span.action-menu-item"/>
    </section>
</sections>
