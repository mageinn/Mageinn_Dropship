        </item>
        <item name="label" xsi:type="string" translate="true">Vendor</item>
    </argument>
    <settings>
        <buttons>
            <button name="back">
                <url path="*/*/"/>
                <class>back</class>
                <label translate="true">Back</label>
            </button>
            <button name="reset">
                <label translate="true">Reset</label>
            </button>
            <button name="delete" class="Mageinn\Dropship\Block\Adminhtml\Edit\DeleteButton"/>
