            <button name="save" class="Mageinn\Dropship\Block\Adminhtml\Edit\SaveButton"/>
        </buttons>
        <layout>
            <navContainerName>left</navContainerName>
            <type>tabs</type>
        </layout>
        <deps>
            <dep>mageinn_dropship_form.mageinn_dropship_form_data_source</dep>
        </deps>
    </settings>
    <dataSource name="mageinn_dropship_form_data_source">
