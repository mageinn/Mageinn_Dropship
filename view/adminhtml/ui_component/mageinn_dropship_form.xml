    <fieldset name="vendor_users">
        <settings>
            <label translate="true">Vendor Users</label>
        </settings>
        <fieldset name="vendor_users_association">
            <settings>
                <label translate="true">Users</label>
                <collapsible>true</collapsible>
                <additionalClasses>
                    <class name="vendor-users">true</class>
                </additionalClasses>
            </settings>
            <container name="vendor_user_association_block" >
                <argument name="data" xsi:type="array">
                    <item name="config" xsi:type="array">
                        <item name="sortOrder" xsi:type="number">10</item>
                    </item>
                </argument>
                <htmlContent name="html_content">
                    <argument name="block" xsi:type="object">Mageinn\Dropship\Block\Adminhtml\Users\Edit\AssignUser</argument>
                </htmlContent>
            </container>
        </fieldset>
    </fieldset>
    <fieldset name="batch_export">
        <settings>
            <label translate="true">Batch Orders Export</label>
            <dataScope/>
        </settings>
        <fieldset name="batch_export_general">
            <settings>
                <label translate="true">General</label>
                <collapsible>true</collapsible>
                <additionalClasses>
                    <class name="batch-export">true</class>
                </additionalClasses>
            </settings>
            <field name="batch_export_enabled" formElement="checkbox">
                <argument name="data" xsi:type="array">
                    <item name="config" xsi:type="array">
                        <item name="source" xsi:type="string">mageinn_dropship</item>
                        <item name="default" xsi:type="number">0</item>
                        <item name="label" xsi:type="string" translate="true">Enable Export</item>
                        <item name="notice" xsi:type="string" translate="true">Yes for auto scheduled, No - disabled</item>
                    </item>
                </argument>
                <settings>
                    <dataType>boolean</dataType>
                </settings>
                <formElements>
                    <checkbox>
                        <settings>
                            <valueMap>
                                <map name="false" xsi:type="number">0</map>
                                <map name="true" xsi:type="number">1</map>
                            </valueMap>
                            <prefer>toggle</prefer>
                        </settings>
                    </checkbox>
                </formElements>
            </field>
            <field name="batch_export_shipment_status" formElement="select">
                <argument name="data" xsi:type="array">
                    <item name="config" xsi:type="array">
                        <item name="source" xsi:type="string">mageinn_dropship</item>
                        <item name="label" xsi:type="string" translate="true">Change Shipment Status On Export</item>
                        <item name="notice" xsi:type="string" translate="true">Set the status of shipment in system after it is exported.</item>
                    </item>
                </argument>
                <settings>
                    <dataType>text</dataType>
                </settings>
                <formElements>
                    <select>
                        <settings>
                            <options class="Mageinn\Dropship\Model\Source\ShipmentStatus"/>
                        </settings>
                    </select>
                </formElements>
            </field>
            <field name="batch_export_schedule" formElement="input">
                <argument name="data" xsi:type="array">
                    <item name="config" xsi:type="array">
                        <item name="source" xsi:type="string">mageinn_dropship</item>
                        <item name="label" xsi:type="string" translate="true">Schedule</item>
                        <item name="notice" xsi:type="string" translate="true">Specify how frequently to export orders. Use manual: https://en.wikipedia.org/wiki/Cron</item>
                    </item>
                </argument>
                <settings>
                    <validation>
                        <rule name="validate-cron" xsi:type="boolean">true</rule>
                    </validation>
                </settings>
            </field>
            <field name="batch_export_destination" formElement="textarea">
                <argument name="data" xsi:type="array">
                    <item name="config" xsi:type="array">
                        <item name="source" xsi:type="string">mageinn_dropship</item>
                        <item name="label" xsi:type="string" translate="true">Destination</item>
                        <item name="notice" xsi:type="string" translate="true">Specify path on server, email or sFTP ('sftp://username@endpoint/path/file') where batch file with orders will be exported to. Available placeholders: [YYYY], [YY], [MM], [DD], [hh], [mm], [ss].</item>
                    </item>
                </argument>
            </field>
            <field name="batch_export_headings" formElement="textarea">
                <argument name="data" xsi:type="array">
                    <item name="config" xsi:type="array">
                        <item name="source" xsi:type="string">mageinn_dropship</item>
                        <item name="label" xsi:type="string" translate="true">Headings in export file</item>
                        <item name="notice" xsi:type="string" translate="true">Provide comma separated headings in preferred order or leave empty if headings are not required</item>
                    </item>
                </argument>
            </field>
            <field name="batch_export_values" formElement="textarea">
                <argument name="data" xsi:type="array">
                    <item name="config" xsi:type="array">
                        <item name="source" xsi:type="string">mageinn_dropship</item>
                        <item name="label" xsi:type="string" translate="true">Values in export file</item>
                        <item name="notice" xsi:type="string" translate="true">Provide comma separated values in preferred order.</item>
                    </item>
                </argument>
            </field>
            <field name="batch_export_private_key" formElement="textarea">
                <argument name="data" xsi:type="array">
                    <item name="config" xsi:type="array">
                        <item name="source" xsi:type="string">mageinn_dropship</item>
                        <item name="label" xsi:type="string" translate="true">SFTP Private Key</item>
                    </item>
                </argument>
            </field>
        </fieldset>
    </fieldset>
    <fieldset name="batch_import">
        <settings>
            <label translate="true">Batch Orders Import</label>
            <dataScope/>
        </settings>
        <fieldset name="batch_import_general">
            <settings>
                <label translate="true">General</label>
                <collapsible>true</collapsible>
                <additionalClasses>
                    <class name="batch-import">true</class>
                </additionalClasses>
            </settings>
            <field name="batch_import_enabled" formElement="checkbox">
                <argument name="data" xsi:type="array">
                    <item name="config" xsi:type="array">
                        <item name="source" xsi:type="string">mageinn_dropship</item>
                        <item name="default" xsi:type="number">0</item>
                        <item name="label" xsi:type="string" translate="true">Enable Import</item>
                        <item name="notice" xsi:type="string" translate="true">Yes for auto scheduled, No - disabled</item>
                    </item>
                </argument>
                <settings>
                    <dataType>boolean</dataType>
                </settings>
                <formElements>
                    <checkbox>
                        <settings>
                            <valueMap>
                                <map name="false" xsi:type="number">0</map>
                                <map name="true" xsi:type="number">1</map>
                            </valueMap>
                            <prefer>toggle</prefer>
                        </settings>
                    </checkbox>
                </formElements>
            </field>
            <field name="batch_import_schedule" formElement="input">
                <argument name="data" xsi:type="array">
                    <item name="config" xsi:type="array">
                        <item name="source" xsi:type="string">mageinn_dropship</item>
                        <item name="label" xsi:type="string" translate="true">Schedule</item>
                        <item name="notice" xsi:type="string" translate="true">Specify how frequently to import orders. Use manual: https://en.wikipedia.org/wiki/Cron</item>
                    </item>
                </argument>
                <settings>
                    <validation>
                        <rule name="validate-cron" xsi:type="boolean">true</rule>
                    </validation>
                </settings>
            </field>
            <field name="batch_import_source" formElement="input">
                <argument name="data" xsi:type="array">
                    <item name="config" xsi:type="array">
                        <item name="source" xsi:type="string">mageinn_dropship</item>
                        <item name="label" xsi:type="string" translate="true">Source location</item>
                        <item name="notice" xsi:type="string" translate="true">The location to import orders from. Local or sFTP ('sftp://username@endpoint/path/file')</item>
                    </item>
                </argument>
            </field>
            <field name="batch_import_file_data" formElement="input">
                <argument name="data" xsi:type="array">
                    <item name="config" xsi:type="array">
                        <item name="source" xsi:type="string">mageinn_dropship</item>
                        <item name="label" xsi:type="string" translate="true">Values in import file</item>
                        <item name="notice" xsi:type="string" translate="true">Provide comma separated values in preferred order</item>
                    </item>
                </argument>
            </field>
            <field name="batch_import_delimiter" formElement="input">
                <argument name="data" xsi:type="array">
                    <item name="config" xsi:type="array">
                        <item name="source" xsi:type="string">mageinn_dropship</item>
                        <item name="label" xsi:type="string" translate="true">Field delimiter</item>
                        <item name="notice" xsi:type="string" translate="true">Provide separator to delimit values in export file</item>
                    </item>
                </argument>
            </field>
        </fieldset>
    </fieldset>
</form>
